{
  "compilerOptions": {
    "target": "es6",
    "module": "es2015",
    "outDir": "js",
    "sourceMap": true,
    "rootDir": "ts",
    "experimentalDecorators": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "es2015",
      "es2016"
    ],
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "importHelpers": true,
    "strict": true,
    "noImplicitReturns": true
  },
  "compileOnSave": false
}